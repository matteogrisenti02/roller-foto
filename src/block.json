{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/roller-foto",
	"version": "1.0.0",
	"title": "Roller Foto",
	"category": "widgets",
	"description": "Simpler Roller of Images",
	"example": {},
	"attributes": {
		"images": {
			"type": "string",
			"default": ""
		},
		"fallbackCurrentYear": {
			"type": "string"
		},
		"showStartingYear": {
			"type": "boolean"
		},
		"startingYear": {
			"type": "string"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "roller-foto",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
